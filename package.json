{
  "name": "ai-agent",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "clean": "npm run clean --workspaces",
    "dev": "npm run dev --workspaces",
    "start": "npm run start --workspaces",
    "start:cli": "npm run start --workspace=@ai-agent/cli --",
    "start:web": "npm run dev --workspace=@ai-agent/web",
    "dev:all": "concurrently \"npm run dev --workspace=@ai-agent/sdk\" \"npm run dev --workspace=@ai-agent/web\""
  },
  "dependencies": {
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
